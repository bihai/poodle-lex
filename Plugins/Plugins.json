{
    "Version": 1,
    "Default": "freebasic",
    "Plugins": {
        "freebasic": {
            "Description": "FreeBasic programming language using proprietary Unicode streams",
            "Source": "FreeBasic/FreeBasic.py",
            "Dependencies": ["FreeBasic/VariableFormatter.py", "FreeBasic/StateMachine.py"],
            "Files": "FreeBasic/Template"
        },
        "cpp": {
            "Description": "C++ programming language interface which consumes an std::istream",
            "Source": "CPlusPlus/CPlusPlus.py",
            "Dependencies": ["CPlusPlus/VariableFormatter.py", "CPlusPlus/StateMachine.py"],
            "Files": "CPlusPlus/Template"
        },
        "dot": {
            "Description": "A graph in the GraphViz dot format.",
            "Source": "Dot/Dot.py",
            "Files": "Dot/Template",
            "Forms": ["nfa", "dfa"],
            "DefaultForm": "dfa"
        },
        "xml": {
            "Description": "XML serialization of the lexical analyzer intermediate formats",
            "Source": "Xml/Xml.py",
            "Dependencies": ["Xml/ElementTreeFactory.py"],
            "Files": "Xml/Template",
            "Forms": ["nfa", "dfa"],
            "DefaultForm": "dfa"
        }
    }
}
